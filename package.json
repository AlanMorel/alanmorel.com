{
    "name": "alanmorel",
    "version": "1.0.0",
    "author": "Alan Morel",
    "license": "MIT",
    "private": true,
    "description": "AlanMorel.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/AlanMorel/alanmorel.com.git"
    },
    "scripts": {
        "watch:development": "vue-cli-service build --mode development --dest dist/development --target app --watch",
        "build:development": "vue-cli-service build --mode development --dest dist/development --target app",
        "build:production": "vue-cli-service build --mode production --dest dist/production --target app --modern",
        "build:server": "tsc --project ./server/tsconfig.json && tsc-alias -p ./server/tsconfig.json",
        "lint:app": "eslint ./src --fix",
        "lint:server": "eslint ./server --fix",
        "server:watch": "nodemon --config ./nodemon.config.json server/core/Server.ts --watch server",
        "server:run": "node ./dist/server/core/Server.js"
    },
    "dependencies": {
        "core-js": "^3.21.0",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.6",
        "express": "^4.17.2",
        "express-fileupload": "^1.3.1",
        "helmet": "^5.0.2",
        "picocolors": "^1.0.0",
        "vue": "^3.2.31",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.17.0",
        "@babel/preset-typescript": "^7.16.7",
        "@types/ejs": "^3.1.0",
        "@types/express": "^4.17.13",
        "@types/express-fileupload": "^1.2.2",
        "@types/node": "^17.0.17",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "@vue/cli-plugin-babel": "^5.0.0-rc.3",
        "@vue/cli-plugin-eslint": "^5.0.0-rc.3",
        "@vue/cli-plugin-router": "^5.0.0-rc.3",
        "@vue/cli-plugin-typescript": "^5.0.0-rc.3",
        "@vue/cli-service": "^5.0.0-rc.3",
        "@vue/compiler-sfc": "^3.2.22",
        "@vue/eslint-config-typescript": "^10.0.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-vue": "^8.4.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "sass": "^1.49.7",
        "sass-loader": "^12.4.0",
        "ts-node": "^10.5.0",
        "tsc-alias": "^1.6.0",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.5"
    }
}
