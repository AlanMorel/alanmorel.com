{
    "name": "alanmorel",
    "version": "0.1.0",
    "author": "Alan Morel",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/AlanMorel/alanmorel.com.git"
    },
    "scripts": {
        "watch-development": "vue-cli-service build --mode development --dest dist/development --target app --watch",
        "build-development": "vue-cli-service build --mode development --dest dist/development --target app",
        "build-production": "vue-cli-service build --mode production --dest dist/production --target app --modern",
        "build-server": "tsc --project ./server/tsconfig.json && tsc-alias -p ./server/tsconfig.json",
        "lint-app": "eslint ./src --fix",
        "lint-server": "eslint ./server --fix",
        "server": "nodemon --config ./nodemon.config.json server/core/Server.ts --watch server",
        "server-static": "yarn run build-server && node ./dist/server/core/Server.js"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "core-js": "^3.18.3",
        "dotenv": "^10.0.0",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.1",
        "vue": "^3.2.0-beta.8",
        "vue-router": "^4.0.0-rc.6"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.15.0",
        "@types/ejs": "^3.1.0",
        "@types/express": "^4.17.13",
        "@types/express-fileupload": "^1.1.7",
        "@types/node": "^16.11.2",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@vue/cli-plugin-babel": "^5.0.0-beta.6",
        "@vue/cli-plugin-eslint": "^5.0.0-beta.6",
        "@vue/cli-plugin-router": "^5.0.0-beta.6",
        "@vue/cli-plugin-typescript": "^5.0.0-beta.6",
        "@vue/cli-service": "^5.0.0-beta.6",
        "@vue/compiler-sfc": "^3.2.20",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^8.0.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-vue": "^7.20.0",
        "nodemon": "^2.0.14",
        "prettier": "^2.4.1",
        "sass": "^1.43.2",
        "sass-loader": "^12.2.0",
        "ts-node": "^10.3.1",
        "tsc-alias": "^1.3.10",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.4"
    }
}
