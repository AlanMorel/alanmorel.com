{
    "name": "alanmorel",
    "version": "1.0.0",
    "author": "Alan Morel",
    "license": "MIT",
    "private": true,
    "description": "AlanMorel.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/AlanMorel/alanmorel.com.git"
    },
    "scripts": {
        "watch:development": "vue-cli-service build --mode development --dest dist/development --target app --watch",
        "build:development": "vue-cli-service build --mode development --dest dist/development --target app",
        "build:production": "vue-cli-service build --mode production --dest dist/production --target app --modern",
        "build:server": "tsc --project ./server/tsconfig.json && tsc-alias -p ./server/tsconfig.json",
        "lint:app": "eslint ./src --fix",
        "lint:server": "eslint ./server --fix",
        "server:watch": "nodemon --config ./nodemon.config.json server/core/Server.ts --watch server",
        "server:run": "node ./dist/server/core/Server.js"
    },
    "dependencies": {
        "core-js": "^3.23.4",
        "dotenv": "^16.0.1",
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "express-fileupload": "^1.4.0",
        "helmet": "^5.1.0",
        "picocolors": "^1.0.0",
        "vue": "^3.2.37",
        "vue-router": "^4.1.2"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.18.2",
        "@babel/preset-typescript": "^7.18.6",
        "@types/ejs": "^3.1.1",
        "@types/express": "^4.17.13",
        "@types/express-fileupload": "^1.2.2",
        "@types/node": "^18.0.3",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-plugin-eslint": "^5.0.8",
        "@vue/cli-plugin-router": "^5.0.8",
        "@vue/cli-plugin-typescript": "^5.0.8",
        "@vue/cli-service": "^5.0.8",
        "@vue/compiler-sfc": "^3.2.37",
        "@vue/eslint-config-typescript": "^11.0.0",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-security": "^1.5.0",
        "eslint-plugin-vue": "^9.2.0",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "sass": "^1.53.0",
        "sass-loader": "^13.0.2",
        "ts-node": "^10.8.2",
        "tsc-alias": "^1.6.11",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.4"
    }
}
