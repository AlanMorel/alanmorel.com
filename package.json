{
    "name": "alanmorel",
    "version": "0.1.0",
    "author": "Alan Morel",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/AlanMorel/alanmorel.com.git"
    },
    "scripts": {
        "watch-dev": "vue-cli-service build --mode development --dest dist/development --target app --watch",
        "build-development": "vue-cli-service build --mode development --dest dist/development --target app",
        "build-production": "vue-cli-service build --mode production --dest dist/production --target app --modern",
        "build-server": "tsc --project ./server/tsconfig.json && tscpaths -p ./server/tsconfig.json -s ./server -o ./dist/server",
        "lint-app": "eslint ./src --fix",
        "lint-server": "eslint ./server --fix",
        "server": "nodemon --config ./nodemon.config.json server/core/Server.ts --watch server",
        "server-static": "yarn run build-server && node ./dist/server/core/Server.js"
    },
    "dependencies": {
        "core-js": "^3.17.2",
        "dotenv": "^10.0.0",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.1",
        "vue": "^3.2.0-beta.8",
        "vue-router": "^4.0.0-rc.6"
    },
    "devDependencies": {
        "@types/ejs": "^3.1.0",
        "@types/express": "^4.17.13",
        "@types/node": "^16.7.10",
        "@types/express-fileupload": "^1.1.7",
        "@typescript-eslint/eslint-plugin": "^4.31.0",
        "@typescript-eslint/parser": "^4.31.0",
        "@vue/cli-plugin-babel": "^5.0.0-beta.3",
        "@vue/cli-plugin-eslint": "^5.0.0-beta.3",
        "@vue/cli-plugin-router": "^5.0.0-beta.3",
        "@vue/cli-plugin-typescript": "^5.0.0-beta.3",
        "@vue/cli-service": "^5.0.0-beta.3",
        "@vue/compiler-sfc": "^3.2.10",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^7.0.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-vue": "^7.17.0",
        "nodemon": "^2.0.12",
        "prettier": "^2.3.2",
        "sass": "^1.39.0",
        "sass-loader": "^12.1.0",
        "ts-node": "^10.2.1",
        "tsc-alias": "^1.3.9",
        "tsconfig-paths": "^3.11.0",
        "tscpaths": "^0.0.9",
        "typescript": "^4.4.2"
    }
}
